{
  "name": "@jupyterlab/latex",
  "version": "1.0.0-pre.1",
  "description": "JupyterLab extension for running LaTeX",
  "author": "M Pacer & Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "latex"
  ],
  "jupyterlab": {
    "extension": "lib/index.js",
    "schemaDir": "schema"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.8",
    "@jupyterlab/apputils": "^1.0.0-alpha.8",
    "@jupyterlab/codeeditor": "^1.0.0-alpha.8",
    "@jupyterlab/coreutils": "^3.0.0-alpha.8",
    "@jupyterlab/docmanager": "^1.0.0-alpha.8",
    "@jupyterlab/docregistry": "^1.0.0-alpha.8",
    "@jupyterlab/fileeditor": "^1.0.0-alpha.8",
    "@jupyterlab/services": "^4.0.0-alpha.8",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/domutils": "^1.1.2",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "pdfjs-dist": "2.0.943",
    "react": "~16.4.2",
    "react-dom": "~16.4.2"
  },
  "devDependencies": {
    "@types/react": "~16.4.13",
    "@types/react-dom": "~16.0.7",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.5.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-latex.git"
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-latex/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyterlab-latex"
}
